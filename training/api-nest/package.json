{
  "name": "api-nest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "prisma": {
    "seed": "ts-node prisma/seeds/seed.ts"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "ng": "nest generate resource",
    "tc": "yarn typeorm migration:create -n CreateItem",
    "tg": "yarn typeorm migration:generate --pretty",
    "tg2": "yarn typeorm migration:generate --pretty migrations/InitialSchema",
    "tr": "yarn typeorm migration:run",
    "ts": "yarn typeorm migration:show",
    "td": "yarn typeorm schema:drop",
    "tss": "yarn typeorm schema:sync",
    "typeorm": "typeorm-ts-node-commonjs -d ./ormconfig.ts",
    "cg": "yarn ts-node ./generate-typing.ts",
    "cgf": "graphql-codegen",
    "prisma": "dotenv -e .env.development prisma",
    "prisma:generate": "yarn prisma generate",
    "prisma:migrate": "yarn prisma migrate dev",
    "prisma:seed": "yarn prisma db seed",
    "prisma:pull": "yarn prisma db pull",
    "pg": "yarn prisma generate",
    "pm": "yarn prisma migrate dev",
    "ps": "yarn prisma db seed",
    "pp": "yarn prisma db pull",
    "seed:config": "yarn ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "yarn ts-node ./node_modules/typeorm-seeding/dist/cli.js --configName ormconfig.ts seed",
    "seed:run2": "yarn ts-node ./node_modules/typeorm-extension/dist/cli/index.js seed",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "ts-node": "ts-node -r tsconfig-paths/register"
  },
  "dependencies": {
    "@apollo/gateway": "^2.4.8",
    "@apollo/server": "^4.7.5",
    "@faker-js/faker": "^8.0.2",
    "@nestjs/apollo": "^12.0.7",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/graphql": "^12.0.7",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "@ntegral/nestjs-sentry": "^4.0.0",
    "@prisma/client": "4.16.2",
    "@sentry/node": "^7.58.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "express-session": "^1.17.3",
    "faker": "^6.6.6",
    "graphql": "^16.7.1",
    "graphql-type-json": "^0.3.2",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "ts-morph": "^19.0.0",
    "typeorm": "^0.3.17",
    "typeorm-seeding": "^1.6.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/named-operations-object": "^2.3.1",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@madsci/graphql-codegen-nestjs": "^0.0.3",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/faker": "^6.6.9",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.4.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "prisma": "^4.16.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
